<div class="row">
    <div class="col-xs-6 col-xs-push-3">
        <h1>
          {{=it.message}}
        </h1>
        <ul class="list-group">
          {{ for (var i = 0; i < 7; i++) { }}
            <li class="list-group-item">
              <span class="badge">{{=it.pkg[it.pkgs[i]]}}</span>
              {{=it.pkgs[i]}}
            </li>
          {{ } }}
        </ul>
    </div>
</div>
